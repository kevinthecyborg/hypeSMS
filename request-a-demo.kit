<!doctype html>
<html lang="en">
  <head>
    <!-- @include includes/head.html -->

    <title>Request a Demo - CheckedOut</title>
  </head>
  <body>

    <!-- CONTENT
    ================================================== -->
    <section class="section-border border-primary">
      <div class="container d-flex flex-column">
        <div class="row align-items-center justify-content-center no-gutters min-vh-100">
          <div class="col-12 col-md-5 col-lg-4 py-8 py-md-11">

            <!-- Logo -->
            <div class="text-center mb-4">
              <img src="assets/img/CheckedOut-logo.svg" alt="CheckedOut Logo" style="width: 250px;">
            </div>

            <!-- Text -->
            <p class="mb-6 text-center text-muted">
              Enter your info to book a demo with us.
            </p>

            <!-- Form -->
            <form class="mb-6">

              <!-- Name -->
              <div class="form-group mb-5">
                <input type="text" class="form-control" id="name" placeholder="Your Name">
              </div>

              <!-- Email -->
              <div class="form-group">
                <input type="email" class="form-control" id="email" placeholder="Business Email Address">
              </div>

              <!-- Phone -->
              <div class="form-group mb-5">
                <input type="phone" class="form-control" id="phone" placeholder="Phone Number">
              </div>

              <!-- Company Name -->
              <div class="form-group mb-5">
                <input type="text" class="form-control" id="companyName" placeholder="Company Name">
              </div>

              <!-- Submit -->
              <button class="btn btn-block btn-primary shadow lift" type="submit">
                Start Booking
              </button>

            </form>

          </div>
        </div> <!-- / .row -->
      </div> <!-- / .container -->
    </section>

    <!-- JAVASCRIPT
    ================================================== -->
    <!-- @include includes/javascript.html -->

  </body>
</html>
